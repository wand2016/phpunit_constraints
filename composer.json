{
    "name": "wand_ta/phpunit_constraints",
    "description": "My Own PHPUnit extensions.",
    "type": "library",
    "require": {
        "phpunit/phpunit": "^8.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daiki Horiyama",
            "email": "d19921207@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^0.11.19"
    },
    "scripts": {
        "lint": [
            "@php ./vendor/bin/phpstan analyse --level max src tests"
        ],
        "test": [
            "@php ./vendor/bin/phpunit tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "WandTa\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
